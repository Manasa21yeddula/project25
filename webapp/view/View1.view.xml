<mvc:View controllerName="project25.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" >
    <Page id="page" title="MY FIRST VIEW">
    <f:SimpleForm id="_IDGenSimpleForm1"  title="Employee Information"  editable ="true" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" >
    <f:content>
    
    <Label  text="EmployeeId" required="true"></Label>
    <Input id="Manasa" ></Input>
    <Label  text="EmployeeName" required="true"></Label>
    <Input id="_IDGenInput2" ></Input>
    <Label  text="Employeeemail" required="true"></Label>
    <Input id="_IDGenInput3" ></Input>
    <Label  text="EmployeeNo" required="true"></Label>
    <Input id="_IDGenInput4" ></Input>
   
    <Label  text="Employeeskill" required="true"></Label>
    <Input id="_IDGenInput5" ></Input>
    <Label  text="Employeedob" required="true"></Label>
    <Input id="_IDGenInput6" ></Input>
    <Label text="EmployeeJoining" required="true"></Label>
    <Input id="_IDGenInput7" ></Input>
    <Label text="EmployeeADRESS" required="true"></Label>
    <Input id="_IDGenInput8" ></Input>
    </f:content>
    </f:SimpleForm>
    <footer>
    <Bar id="_IDGenBar1">
    <contentRight>
    <Button id="_IDGenButton1" text="Submit" press="onSubmit" type="Emphasized"/>
    </contentRight>
    </Bar>
    </footer>
</Page>
</mvc:View>



